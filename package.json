{
  "name": "anxietyCBT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "ip7": "react-native run-ios --simulator 'iPhone 7'",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && yarn install",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "eslint src/",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s"
  },
  "pre-commit": "lint",
  "dependencies": {
    "@ant-design/icons-react-native": "^1.0.2",
    "@ant-design/react-native": "^3.1.8",
    "axios": "^0.19.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "0.59.8",
    "react-native-card-stack-swiper": "^1.1.0",
    "react-native-chart-kit": "^2.6.1",
    "react-native-config": "^0.11.7",
    "react-native-datepicker": "^1.7.2",
    "react-native-deck-swiper": "1.5.25",
    "react-native-dotenv": "^0.2.0",
    "react-native-drawer": "^2.5.1",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-i18n": "^2.0.15",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-modal-datetime-picker": "^7.4.2",
    "react-native-pdf": "^5.1.0",
    "react-native-picker-select": "^6.1.1",
    "react-native-progress": "^3.6.0",
    "react-native-push-notification": "^3.1.3",
    "react-native-snap-carousel": "3.7.5",
    "react-native-svg": "^9.4.0",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^3.11.0",
    "react-navigation-redux-helpers": "^3.0.2",
    "react-redux": "^7.0.3",
    "realm": "2.27.0",
    "redux": "4.0.1",
    "redux-form": "8.2.3",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "1.0.2",
    "reduxsauce": "1.1.0",
    "reselect": "^4.0.0",
    "rn-placeholder": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-react-require": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "16.8.3",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
